public class Operators
{
		public static void main(String [] args)
		
		{
			int x=10; 
			int y=20;
			boolean a=true;
			boolean b=false;
			  
			System.out.println(x++); 
            System.out.println(++x);  
			System.out.println(x--);  
			System.out.println(--x);
			System.out.println(x++ + y++);
			System.out.println(x-- + y--); 
			System.out.println(x++ + y--);
			System.out.println(x-- - y--);
			System.out.println(x-- + y--);
			System.out.println(~x);
			System.out.println(~y);
			System.out.println(!a);
			System.out.println(!b);
			System.out.println(x+y);
			System.out.println(x-y);
			System.out.println(x*y);
			System.out.println(x/y);
			int min=(x<y)?x:y;  
			System.out.println(min); 
			}	
}		
	
  
